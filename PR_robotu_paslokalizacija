def measurement():
    i = 0
    for x in karte:
        if karte[i] != "X":
            if m == "s":
                z = 1
                if directionZ(i, z) == "X":
                    karteP1[i] = karteN1[i] * ss
                if directionZ(i, z) != "X":
                    karteP1[i] = karteN1[i] * sb
                z = 2
                if directionZ(i, z) == "X":
                    karteP2[i] = karteN2[i] * ss
                if directionZ(i, z) != "X":
                    karteP2[i] = karteN2[i] * sb
                z = 3
                if directionZ(i, z) == "X":
                    karteP3[i] = karteN3[i] * ss
                if directionZ(i, z) != "X":
                    karteP3[i] = karteN3[i] * sb
                z = 4
                if directionZ(i, z) == "X":
                    karteP4[i] = karteN4[i] * ss
                if directionZ(i, z) != "X":
                    karteP4[i] = karteN4[i] * sb
            elif m == "b":
                z = 1
                if directionZ(i, z) == "X":
                    karteP1[i] = karteN1[i] * bs
                if directionZ(i, z) != "X":
                    karteP1[i] = karteN1[i] * bb
                z = 2
                if directionZ(i, z) == "X":
                    karteP2[i] = karteN2[i] * bs
                if directionZ(i, z) != "X":
                    karteP2[i] = karteN2[i] * bb
                z = 3
                if directionZ(i, z) == "X":
                    karteP3[i] = karteN3[i] * bs
                if directionZ(i, z) != "X":
                    karteP3[i] = karteN3[i] * bb
                z = 4
                if directionZ(i, z) == "X":
                    karteP4[i] = karteN4[i] * bs
                if directionZ(i, z) != "X":
                    karteP4[i] = karteN4[i] * bb
        i += 1

def directionZ(i, z):
    if z == 1:
        i = i + 1
    elif z == 2:
        i = i - 12
    elif z == 3:
        i = i - 1 
    elif z == 4:
        i = i + 12
    return karte[i]

def normalization():
    i = 0
    sum = 0
    
    for x in karte:
        if karte[i] != "X":
            #print(i, sum, karteP[i], type(karteP[i]))
            sum = sum + karteP1[i] + karteP2[i] + karteP3[i] + karteP4[i]
        i += 1
    
    #print(sum)
    i = 0
    for x in karte:
        if karte[i] != "X":
            karteN1[i] = karteP1[i] / sum
            karteN2[i] = karteP2[i] / sum
            karteN3[i] = karteP3[i] / sum
            karteN4[i] = karteP4[i] / sum
        i += 1
    
def move():
    i = 0
    
    for x in karte:
        if karte[i] != "X":
            if p == "l":
                karteP1[i] = karteN1[i] * n + karteN2[i] * v
                karteP2[i] = karteN2[i] * n + karteN3[i] * v
                karteP3[i] = karteN3[i] * n + karteN4[i] * v
                karteP4[i] = karteN4[i] * n + karteN1[i] * v
            elif p == "k":
                karteP1[i] = karteN1[i] * n + karteN4[i] * v
                karteP2[i] = karteN2[i] * n + karteN1[i] * v
                karteP3[i] = karteN3[i] * n + karteN2[i] * v
                karteP4[i] = karteN4[i] * n + karteN3[i] * v  
            elif p == "t":
                if directionZ(i, 1) != "X":
                    karteP3[i] = karteN3[i] * n + karteN3[i+1] * v
                else:
                    karteP3[i] = karteN3[i] * n
                
                if directionZ(i, 2) != "X":
                    karteP4[i] = karteN4[i] * n + karteN4[i-12] * v
                else:
                    karteP4[i] = karteN4[i] * n
                
                if directionZ(i, 3) != "X":
                    karteP1[i] = karteN1[i] * n + karteN1[i-1] * v
                else:
                    karteP1[i] = karteN1[i] * n
                
                if directionZ(i, 4) != "X":
                    karteP2[i] = karteN2[i] * n + karteN2[i+12] * v
                else:
                    karteP2[i] = karteN2[i] * n
                
        i += 1

print("Vides karte:")
printMap()

print("Inicializācija")
#printProbability()

